<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TableView fx:id="recordTableView" prefHeight="150.0" prefWidth="400">
          <columns>
              <TableColumn fx:id="nameColumn" text="Name"/>
          </columns>
      </TableView>
      <Button fx:id="clickButton" mnemonicParsing="false" text="Button" GridPane.rowIndex="1" />
   </children>
</GridPane>
